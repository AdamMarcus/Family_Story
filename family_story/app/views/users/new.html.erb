<h1>Create an Account</h1>
<% if flash[:notice] %>
    <div class="notice"><%= flash[:notice] %></div>
<% end %>

<%= form_for @user do |user_form| %>
    Username: <%= user_form.text_field :username %> <br />

    <%= user_form.submit 'Create New User'%> <%=link_to "Login Page", users_loginpage_path%>

<% end %>